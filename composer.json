{
    "name": "skaisser/sendy",
    "description": "Sendy API implementation for Laravel 11",
    "keywords": ["laravel", "sendy", "email", "newsletter", "API"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shirleyson Kaisser",
            "email": "skaisser@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Skaisser\\Sendy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skaisser\\Tests\\Sendy\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skaisser\\Sendy\\SendyServiceProvider"
            ],
            "aliases": {
                "Sendy": "Skaisser\\Sendy\\Facades\\Sendy"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}